{
  "name": "express-strater",
  "version": "1.0.0",
  "description": "This template is a quick starter for other project.",
  "main": "src/api/bin",
  "scripts": {
    "tsc": "tsc",
    "start": "cross-env NODE_ENV=production ts-node --files src/api/bin",
    "start-dev": "npm run build && cross-env NODE_ENV=development tsnd --poll --files src/api/bin",
    "start-server-dev": "cross-env NODE_ENV=development tsnd --poll --files src/api/bin",
    "build": "vite build",
    "start-test": "cross-env NODE_ENV=test tsnd --files src/api/bin",
    "jest": "jest",
    "jest-cover": "jest --coverage",
    "migrate-up": "npx migrate up",
    "migrate-down": "npx migrate down {$migrate-name}",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamadalikaboosi/express-starter.git"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "microservice"
  ],
  "author": "samb2",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohamadalikaboosi/express-starter/issues"
  },
  "homepage": "https://github.com/mohamadalikaboosi/express-starter#readme",
  "dependencies": {
    "@dropb/ffprobe": "^3.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node-cron": "^3.0.7",
    "@types/underscore": "^1.11.4",
    "auto-bind": "^4.0.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "input": "^1.0.1",
    "irolegroup": "^1.0.5",
    "jnaudiostream": "^1.0.2",
    "mongoose": "^7.0.5",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.4.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.5.3",
    "telegram": "^2.16.4",
    "throttle": "^1.0.3",
    "ts-node": "^10.9.1",
    "underscore": "^1.13.6",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.18",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "vite": "^3.1.0"
  }
}
